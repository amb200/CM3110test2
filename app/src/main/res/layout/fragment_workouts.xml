<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/teal_700"
    tools:context=".Workouts">

    <ImageView
        android:id="@+id/bicepsIcon"
        android:layout_width="136dp"
        android:layout_height="144dp"
        android:layout_marginTop="16dp"
        android:src="@mipmap/biceps_foreground"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@mipmap/biceps_foreground" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Today's Workout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/backIcon"
        android:layout_width="138dp"
        android:layout_height="144dp"
        android:src="@mipmap/back2_foreground"
        app:layout_constraintStart_toEndOf="@+id/bicepsIcon"
        app:layout_constraintTop_toTopOf="@+id/bicepsIcon"
        app:srcCompat="@mipmap/back2_foreground" />

    <ImageView
        android:id="@+id/chestIcon"
        android:layout_width="130dp"
        android:layout_height="144dp"
        app:layout_constraintEnd_toStartOf="@+id/bicepsIcon"
        app:layout_constraintTop_toTopOf="@+id/bicepsIcon"
        app:srcCompat="@mipmap/chest_foreground"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Tomorrow"
        app:layout_constraintEnd_toEndOf="@+id/bicepsIcon"
        app:layout_constraintStart_toStartOf="@+id/bicepsIcon"
        app:layout_constraintTop_toBottomOf="@+id/bicepsIcon" />

    <ImageView
        android:id="@+id/tricepsIcon"
        android:layout_width="138dp"
        android:layout_height="145dp"
        android:layout_marginStart="-128dp"
        android:layout_marginTop="16dp"
        android:src="@mipmap/tricep_foreground"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ImageView
        android:id="@+id/shoulderIcon"
        android:layout_width="138dp"
        android:layout_height="145dp"
        android:layout_marginStart="32dp"
        android:src="@mipmap/shoulder_foreground"
        app:layout_constraintStart_toEndOf="@+id/tricepsIcon"
        app:layout_constraintTop_toTopOf="@+id/tricepsIcon" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Day after"
        app:layout_constraintEnd_toStartOf="@+id/shoulderIcon"
        app:layout_constraintStart_toEndOf="@+id/tricepsIcon"
        app:layout_constraintTop_toBottomOf="@+id/tricepsIcon" />

    <ImageView
        android:id="@+id/legsIcon"
        android:layout_width="133dp"
        android:layout_height="143dp"
        android:layout_marginTop="16dp"
        android:src="@mipmap/legs_foreground"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:srcCompat="@mipmap/legs_foreground" />
</androidx.constraintlayout.widget.ConstraintLayout>